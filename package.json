{
	"name": "cpx-language",
	"displayName": "cpx-language",
	"description": "cpx format support for asm-dom spa lib",
	"version": "0.0.1",
	"publisher": "ahmad2smile",
	"engines": {
		"vscode": "^1.22.0"
	},
	"categories": ["Languages"],
	"contributes": {
		"languages": [
			{
				"id": "CPX",
				"aliases": ["CPX"],
				"extensions": [".cpx", ".cpp"],
				"configuration": "./language-configuration.json"
			}
		],
		"grammars": [
			{
				"language": "CPX",
				"scopeName": "source.cpp",
				"path": "./syntaxes/cpx.tmLanguage.json"
			}
		]
	}
}
